# Please check roles/common/vars/leofs_releases for available versions
[all:vars]
leofs_version=1.2.22
build_temp_path="/tmp/leofs_builder"
build_install_path="/tmp/"
build_branch="master"
source="package"

[builder]
192.168.0.10

# nodename of leo_manager_0 and leo_manager_1 are set at group_vars/all
[leo_manager_0]
192.168.0.101

# nodename of leo_manager_0 and leo_manager_1 are set at group_vars/all
[leo_manager_1]
192.168.0.101

[leo_storage]
192.168.0.151 leofs_module_nodename=S0@192.168.0.151
192.168.0.152 leofs_module_nodename=S0@192.168.0.152
#192.168.0.37 leofs_module_nodename=S1@192.168.0.37
#192.168.0.38 leofs_module_nodename=S2@192.168.0.38
#192.168.0.39 leofs_module_nodename=S3@192.168.0.39
#192.168.0.40 leofs_module_nodename=S4@192.168.0.40

[leo_gateway]
192.168.0.201 leofs_module_nodename=G0@192.168.0.201

[leofs_nodes:children]
leo_manager_0
leo_manager_1
leo_gateway
leo_storage
